{
    "name": "samsonasik/naming",
    "type": "library",
    "description": "Naming Filter and Validator for people name",
    "keywords": [
        "naming",
        "people",
        "name",
        "multibyte",
        "mbstring",
        "laminas"
    ],
    "homepage": "https://github.com/samsonasik/Naming",
    "license": "MIT",
    "authors": [
        {
            "name": "Abdul Malik Ikhsan",
            "email": "samsonasik@gmail.com",
            "homepage": "http://samsonasik.wordpress.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "laminas/laminas-filter": "^2.9",
        "laminas/laminas-validator": "^2.12",
        "symfony/polyfill-mbstring": "^1.10",
        "webmozart/assert": "^1.4"
    },
    "require-dev": {
        "kahlan/kahlan": "^4.7.2",
        "laminas/laminas-coding-standard": "^2.0",
        "php-coveralls/php-coveralls": "^2.1",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-webmozart-assert": "^0.12.0",
        "rector/rector": "^0.11.53",
        "squizlabs/php_codesniffer": "^3.5.8"
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Naming\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Naming\\Spec\\": "spec/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf"
    }
}
