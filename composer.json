{
    "name": "samsonasik/naming",
    "type": "library",
    "description": "Naming Filter and Validator for people name",
    "keywords": [
        "naming",
        "people",
        "name",
        "multibyte",
        "mbstring",
        "laminas"
    ],
    "homepage": "https://github.com/samsonasik/Naming",
    "license": "MIT",
    "authors": [
        {
            "name": "Abdul Malik Ikhsan",
            "email": "samsonasik@gmail.com",
            "homepage": "http://samsonasik.wordpress.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "laminas/laminas-filter": "^3.0",
        "laminas/laminas-validator": "^3.0",
        "samsonasik/array-lookup": "^1.3",
        "symfony/polyfill-mbstring": "^1.27",
        "webmozart/assert": "^1.11"
    },
    "require-dev": {
        "kahlan/kahlan": "^5.2.2",
        "laminas/laminas-coding-standard": "^2.5",
        "php-coveralls/php-coveralls": "^2.5.3",
        "phpstan/phpstan": "^2.0",
        "phpstan/phpstan-webmozart-assert": "^2.0",
        "rector/rector": "dev-main",
        "squizlabs/php_codesniffer": "^3.7.1"
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "Naming\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Naming\\Spec\\": "spec/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf"
    }
}
